{{#if resume.projects.length}}
<section id="projects">
    <h2>Projetos</h2>
    <div class="item">
        {{#each resume.projects}}
            {{#if name}}{{#if description}}{{#if url}}
                {{#> item title=name subtitle=description url=url mainUrl='y'}}   
                {{/item}}
            {{/if}}{{/if}}{{/if}}
        {{/each}}
    </div>
</section>
{{/if}}
